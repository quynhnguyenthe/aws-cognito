{
    "name": "quynhnguyenthe/aws-cognito",
    "description": "AWS Cognito package that allows Auth and other related features using the AWS SDK for PHP",
    "keywords": ["php", "laravel", "aws", "cognito", "auth", "authentication", "oauth", "user pool"],
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "aws/aws-sdk-php": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
          "Ellaisys\\Cognito\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Cognito\\Providers\\AwsCognitoServiceProvider"
            ],
            "aliases": {
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
